<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.rebrandsoftware.popuptest" versionCode="1" version="1.0.0">

    <!-- versionCode is optional and Android only -->

    <preference name="phonegap-version" value="3.6.3" />

    <name>PopupTest</name>

    <description>
        An app to test menu popups on Android devices
    </description>

    <author href="http://www.rebrandsoftware.com" email="mike@rebrandsoftware.com">
        Mike Gibson - Rebrand Software, LLC
    </author>

    <access origin="*" />
    
    <feature name="debug-server" required="true">
       <param name="domain" value="http://192.168.1.103:9090"/>
       <param name="key" value="I_DRINK_YOUR_MILKSHAKE"/>
    </feature>

</widget>